{
  "version": 2,
  "framework": "create-react-app",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs@18.x",
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 